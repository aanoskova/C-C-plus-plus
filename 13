//создать массив из 5 элементов со значентями 1 4 -5 6 2
//найти минимумальный элеиент в этом массиве

#include <stdio.h>
int main() {
    int a[5] = { 1,4,-5,6,2 };
    int min = a[0];
    int i = 0;
    while (i < 5) {
        if (a[i] < min) {
            min = a[i];
        }
        i++;
    }
    printf("%d", min);
}
